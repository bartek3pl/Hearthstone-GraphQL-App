{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["eslint:recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "class-methods-use-this": "off",
    "no-restricted-syntax": "off",
    "no-underscore-dangle": "off"
  },
  "globals": {
    "process": true
  }
}
